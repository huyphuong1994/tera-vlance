  "private": true,
  "scripts": {
    "pack": "npm pack",
    "build": "rm -rf lib tera-dls*.tgz && npm run css:build && gulp build-css && rollup -c --context window && npm pack",
    "test-coverage": "react-scripts test --coverage --watchAll --maxWorkers=4 --silent --colors",
    "test": "react-scripts test --watchAll -verbose --silent",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "clean": "del -rf node_modules package-lock.json && npm cache clean --force && npm i",
    "css:build": "sass src/styles/:public/build-css",
    "css:watch": "sass --watch src/styles/:public/build-css",
    "start": "set PORT=3002 && react-scripts start && npm run css:watch",
    "build-demo": "react-scripts build",
    "fix-eslint": "eslint --fix --ext .js,.jsx,ts,tsx .",
    "reinstall": "rm -rf package-lock.json && rm -rf node_modules && npm install"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
